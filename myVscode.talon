#custom vscode commands go here
app: vscode
-
save:
    key(cmd-s)
    
source:
    user.vscode("workbench.view.scm")

search global:
    key(cmd-shift-f)

search down:
    key(f4)

search up:
    key(shift-f4)
    
    
speak:
    key(cmd-k) 

chat:
    key(cmd-l) 

zoom in:
    key(cmd-=)

zoom out:
    key(cmd--)

